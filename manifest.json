{
"manifest_version": 2,
"name": "funswitch",
"description": "This is Funswitch Assignment for the Frond End Developer Intern Role",
"version": "1.0",
"browser_action": {
"default_icon": "icon.png",
"default_popup": "popup.html"
},
"permissions": [
   "activeTab",
    "tabs"
   ],
"content_scripts" :[
	{
		"matches" : ["*://*/*"],
		"js" : ["background.js"]
	}
]  
}